{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static-build"
    },
    {
      "src": "src/server/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/server/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
